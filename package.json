{
  "name": "superelf",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4203",
    "start:nl": "ng serve --configuration=nl",
    "build": "ng build --prod && npm run split-critical-css",
    "build-acc": "ng build --configuration=acc && npm run split-critical-css",
    "build-dev": "ng build",
    "build-dev-nl": "ng build --configuration=production-nl",
    "split-critical-css": "npm run split-critical-css-step1 && npm run split-critical-css-step2",
    "split-critical-css-step1": "cat dist/index.html | critical --base dist/ --inline > dist/index.critical.html",
    "split-critical-css-step2": "rm dist/index.html && mv dist/index.critical.html dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build-bundle-report && webpack-bundle-analyzer dist/stats-es2015.json",
    "build-bundle-report": "ng build --prod --stats-json"
  },
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/forms": "^10.2.0",
    "@angular/localize": "^10.2.0",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@angular/service-worker": "^10.2.0",
    "@fortawesome/angular-fontawesome": "0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "lodash.clonedeep": "^4.5.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-sport": "^1.0.7",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.0",
    "@angular/language-service": "^10.2.0",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.162",
    "@types/node": "^13.13.28",
    "critical": "^1.3.10",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "~4.0.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-dev-server": "^3.11.0"
  }
}